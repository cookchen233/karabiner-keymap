            "from": {"key_code": "tab","modifiers": {"mandatory": ["left_control"]}},
            "to": [{"key_code": "tab","modifiers": ["left_command"]}],
            "conditions": [{"type": "frontmost_application_if","bundle_identifiers": ["^com.microsoft.rdc.osx.beta"]}]
        },{
            "type": "basic",

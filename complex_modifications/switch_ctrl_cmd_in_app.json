            "from": {
                "key_code": "b",
                "modifiers": {
                    "optional": ["any"]
                }
            },
            "to": [{
                "key_code": "page_up",
                "modifiers": ["left_control"]
            }],
            "conditions": [{
                "type": "variable_if",
                "name": "spacefn_mode",
                "value": 1
            },{
                "type": "frontmost_application_if",
                "bundle_identifiers": [
                  "^com.microsoft.rdc.osx.beta"
                ]
            }]

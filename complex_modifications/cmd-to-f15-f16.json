{
    "title": "Cmd to F15,F16",
    "rules": [
        {
            "description": "Cmd to F15,F16 for browser back/forward",
            "manipulators": [
                {
                    "type": "basic",
                    "from": {
                        "key_code": "left_command",
                        "modifiers": {
                            "optional": [
                                "any"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "left_command"
                        }
                    ],
                    "to_if_alone": [
                        {
                            "key_code": "f15"
                        }
                    ],
                    "conditions": [
                        {
                            "type": "frontmost_application_unless",
                            "bundle_identifiers": [
                                "^com.microsoft.rdc.osx.beta"
                            ]
                        }
                    ]
                },
                {
                    "type": "basic",
                    "from": {
                        "key_code": "right_command",
                        "modifiers": {
                            "optional": [
                                "any"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "right_command"
                        }
                    ],
                    "to_if_alone": [
                        {
                            "key_code": "f16"
                        }
                    ],
                    "conditions": [
                        {
                            "type": "frontmost_application_unless",
                            "bundle_identifiers": [
                                "^com.microsoft.rdc.osx.beta"
                            ]
                        }
                    ]
                }
            ]
        }
    ]
}
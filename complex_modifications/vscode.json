{
  "title": "VSCode extra doing while run code",
  "rules": [
    {
      "description": "VSCode extra doing while run code",
      "manipulators": [
        {
          "type": "basic",
          "from": {
              "key_code": "f10"
          },
          "to": [
            {
              "key_code": "f10"
            },
            {
              "shell_command": "~/go/bin/mycli command-log create"
            }
          ],"conditions": [{"type": "frontmost_application_if","bundle_identifiers": ["^com.microsoft.VSCode"]}]
        }
      ]
    }
  ]
}


{
  "title": "To open most often app using Command+e",
  "rules": [
    {
      "description": "Command+e to open Finder",
      "manipulators": [{
          "type": "basic",
          "from": {
              "key_code": "e",
              "modifiers": {
                  "mandatory": [
                    "command"
                  ],
                  "optional": [
                    "any"
                  ]
                }
          },
          "to": [
              {
                  "shell_command": "open -a 'Visual Studio Code.app' ~/Coding/myscript/test.sh"
              }
          ]
      }]
  }
  ]
}


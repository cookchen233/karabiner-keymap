{
  "title": "To open most often app using Command+g",
  "rules": [
    {
      "description": "Command+g to open Finder",
      "manipulators": [{
          "type": "basic",
          "from": {
              "key_code": "g",
              "modifiers": {
                  "mandatory": [
                    "command"
                  ],
                  "optional": [
                    "any"
                  ]
                }
          },
          "to": [
              {
                  "shell_command": "open -a 'Finder.app'"
              }
          ]
      }]
  }
  ]
}


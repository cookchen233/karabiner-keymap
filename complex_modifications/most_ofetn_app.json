{
  "title": "To open most often app using Command+[u/i/o/p]",
  "rules": [
    {
      "description": "Command+u to open Visual Studio Code",
      "manipulators": [{
          "type": "basic",
          "from": {
              "key_code": "u",
              "modifiers": {
                  "mandatory": [
                    "command"
                  ],
                  "optional": [
                    "any"
                  ]
                }
          },
          "to": [
              {
                  "shell_command": "open -a 'Visual Studio Code.app'"
              }
          ]
      }]
  }, {
      "description": "Command+i to active IntelliJ IDEA",
      "manipulators": [{
          "type": "basic",
          "from": {
              "key_code": "i",
              "modifiers": {
                  "mandatory": [
                    "command"
                  ],
                  "optional": [
                    "any"
                  ]
                }
          },
          "to": [
              {
                  "shell_command": "open -a 'IntelliJ IDEA.app'"
              }
          ]
      }]
  }, {
      "description": "Command+o to active Google Chrome",
      "manipulators": [{
          "type": "basic",
          "from": {
              "key_code": "o",
              "modifiers": {
                  "mandatory": [
                    "command"
                  ],
                  "optional": [
                    "any"
                  ]
                }
          },
          "to": [
              {
                  "shell_command": "open -a 'Google Chrome.app'"
              }
          ]
      }]
  }, {
      "description": "Command+p to active Microsoft Remote Desktop Beta",
      "manipulators": [{
          "type": "basic",
          "from": {
              "key_code": "p",
              "modifiers": {
                  "mandatory": [
                    "command"
                  ],
                  "optional": [
                    "any"
                  ]
                }
          },
          "to": [
              {
                  "shell_command": "open -a 'Microsoft Remote Desktop Beta.app'"
              }
          ]
      }]
  }
  ]
}


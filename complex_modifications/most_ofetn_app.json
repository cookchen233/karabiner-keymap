{
  "title": "To open most often app",
  "rules": [
    {
      "description": "Command+e to open Vocode",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "e",
            "modifiers": {
              "mandatory": [
                "command"
              ],
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "shell_command": "open -a 'Visual Studio Code.app' ~/Notebook/todo/todo.md"
            }
          ]
        }
      ]
    },
    {
      "description": "Command+g to open Sequel Ace",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "g",
            "modifiers": {
              "mandatory": [
                "command"
              ],
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "shell_command": "open -a 'Sequel Ace.app'"
            }
          ]
        }
      ]
    },
    {
      "description": "Command+j to open windsurf",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "j",
            "modifiers": {
              "mandatory": [
                "command"
              ],
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "shell_command": "open -a 'Windsurf.app'"
            }
          ]
        }
      ]
    },
    {
      "description": "f2 to long bridge",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "f2"
          },
          "to": [
            {
              "shell_command": "open -a 'Longbridge Pro SG.app'"
            }
          ]
        }
      ]
    },
    {
      "description": "f3 to edge",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "f3"
          },
          "to": [
            {
              "shell_command": "open -a 'MicroSoft Edge.app'"
            }
          ]
        }
      ]
    }
  ]
}
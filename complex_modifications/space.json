{
    "title": "SpaceFN Mode",
    "rules": [
        {
            "description": "SpaceFN: Space enables SpaceFN mode (see: https://geekhack.org/index.php?topic=51069.0)",
            "manipulators": [
                {
                    "type": "basic",
                    "from": {
                        "key_code": "spacebar",
                        "modifiers": {
                            "optional": [
                                "any"
                            ]
                        }
                    },
                    "to": [
                        {
                            "set_variable": {
                                "name": "spacefn_mode",
                                "value": 1
                            }
                        },
                        {
                            "set_variable": {
                                "name": "swap_9_0",
                                "value": 0
                            }
                        }
                    ],
                    "to_if_alone": [
                        {
                            "key_code": "spacebar"
                        }
                    ],
                    "to_after_key_up": [
                        {
                            "set_variable": {
                                "name": "spacefn_mode",
                                "value": 0
                            }
                        }
                    ],
                    "conditions": [
                        {
                            "type": "frontmost_application_unless",
                            "bundle_identifiers": [
                                "^com.microsoft.rdc.osx.beta"
                            ]
                        }
                    ]
                }
            ]
        },
        {
            "description": "SpaceFN: Space + Z to Space (hold to repeat)",
            "manipulators": [
                {
                    "type": "basic",
                    "from": {
                        "key_code": "z",
                        "modifiers": {
                            "optional": [
                                "any"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "spacebar"
                        }
                    ],
                    "conditions": [
                        {
                            "type": "variable_if",
                            "name": "spacefn_mode",
                            "value": 1
                        }
                    ]
                }
            ]
        },
        {
            "description": "SpaceFN: Space + E/D/S/F to Up/Down/Left/Right arrows",
            "manipulators": [
              {
                "type": "basic",
                "from": { "key_code": "e", "modifiers": { "optional": ["any"] } },
                "to": [{ "key_code": "up_arrow" }],
                "conditions": [{ "type": "variable_if", "name": "spacefn_mode", "value": 1 }]
              },
              {
                "type": "basic",
                "from": { "key_code": "d", "modifiers": { "optional": ["any"] } },
                "to": [{ "key_code": "down_arrow" }],
                "conditions": [{ "type": "variable_if", "name": "spacefn_mode", "value": 1 }]
              },
              {
                "type": "basic",
                "from": { "key_code": "s", "modifiers": { "optional": ["any"] } },
                "to": [{ "key_code": "left_arrow" }],
                "conditions": [{ "type": "variable_if", "name": "spacefn_mode", "value": 1 }]
              },
              {
                "type": "basic",
                "from": { "key_code": "f", "modifiers": { "optional": ["any"] } },
                "to": [{ "key_code": "right_arrow" }],
                "conditions": [{ "type": "variable_if", "name": "spacefn_mode", "value": 1 }]
              }
            ]
        },
